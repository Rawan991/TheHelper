<h2>Log in</h2>

   <div class="form-row">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

 <div class="form-row">
    <div class="form-group col-md-4">

    <%= f.label :email  , for:"inputAddress"%><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" ,   class:"form-control" ,id:"inputZip"%>
    </div>
  </div>

 <div class="form-row">
    <div class="form-group col-md-4">

    <%= f.label :password  , for:"inputAddress"%><br />
    <%= f.password_field :password, autocomplete: "current-password" ,   class:"form-control" ,id:"inputZip" %>

    </div>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" , class: "actions" %>
  </div>
<% end %>
</div>
<%= render "users/shared/links" %>
